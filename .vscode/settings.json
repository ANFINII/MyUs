{
  "explorer.sortOrder": "type",
  "explorer.sortOrderLexicographicOptions": "unicode",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.associations": {
    "*.tsx": "typescriptreact"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.openRepositoryInParentFolders": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.editor.decorations.colors": false,
  "workbench.colorTheme": "Default Light+",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.codeActionsOnSave": {
      "source.fixAll": "never",
      "source.organizeImports": "never"
    },
  },
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.defaultFoldingRangeProvider": "dbaeumer.vscode-eslint",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.addMissingImports": "explicit",
  },
  "eslint.debug": true,
  "eslint.useFlatConfig": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "python.languageServer": "None",
  "python.defaultInterpreterPath": "${workspaceFolder}/myus/.venv/bin/python",
  "mypy-type-checker.path": ["${workspaceFolder}/myus/.venv/bin/mypy"],
  "mypy-type-checker.importStrategy": "fromEnvironment",
  "mypy-type-checker.cwd": "${workspaceFolder}/myus",
  "notebook.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.accessibilitySupport": "off",
}
